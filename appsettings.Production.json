{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MongoDbSettings": {
    "ConnectionString": "${MONGODB_CONNECTION_STRING}",
    "DatabaseName": "${MONGODB_DATABASE_NAME}",
    "OrdenesCollectionName": "${MONGODB_ORDENES_COLLECTION_NAME}",
    "MedicamentosCollectionName": "${MONGODB_MEDICAMENTOS_COLLECTION_NAME}",
    "ProcedimientosCollectionName": "${MONGODB_PROCEDIMIENTOS_COLLECTION_NAME}",
    "AyudasDiagnosticasCollectionName": "${MONGODB_AYUDAS_DIAGNOSTICAS_COLLECTION_NAME}"
  }
}
